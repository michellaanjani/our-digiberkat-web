{
  "version": 2,
  "framework": null,
  "functions": {
    "api/index.php": {
      "runtime": "vercel-php@0.7.3"
    }
  },
  "routes": [
    {
      "src": "/build/(.*)",
      "dest": "/public/build/$1"
    },
    {
      "src": "/images/(.*)",
      "dest": "/public/images/$1"
    },
    {
        "src": "/(.*\\.php)",
        "dest": "/api/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/api/index.php"
    }
  ],
  "outputDirectory": "public",

    "env": {
        "APP_NAME": "Laravel",
        "APP_ENV": "production",
        "APP_KEY": "base64:0nU1Ux9swZmHzE3T7YYke1PQNfFOe+CUmXwQOEOq5Xw=",
        "APP_DEBUG": "0",
        "APP_URL": "https://our-digiberkat-webv2.vercel.app",
        "APP_CONFIG_CACHE": "/tmp/config.php",
        "APP_EVENTS_CACHE": "/tmp/events.php",
        "APP_PACKAGES_CACHE": "/tmp/packages.php",
        "APP_ROUTES_CACHE": "/tmp/routes.php",
        "APP_SERVICES_CACHE": "/tmp/services.php",
        "VIEW_COMPILED_PATH": "/tmp",
        "CACHE_DRIVER": "array",
        "LOG_CHANNEL": "stderr",
        "LOG_LEVEL": "info",
        "SESSION_DRIVER": "cookie",
        "VITE_APP_NAME": "Laravel"
    }

}
